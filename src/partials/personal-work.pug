section.section.section--personal-work
  .l-container
    h2.section__heading
      span.section__headingInner personal works
  .work
    ul.js-slick-personal
      - works = {};
      - works[0] = {title: 'schoo tours', src: "schoo-tours.jpg", link: 'schoo-tours.html'};
      - works[1] = {title: 'Feed Pocket', src: "feed-pocket.jpg", link: 'feed-pocket.html'};
      - works[2] = {title: 'timeline', src: "timeline.png", link: 'timeline.html'};
      - works[3] = {title: 'ballxball', src: "ballxball.png", link: 'ballxball.html'};
      each work, i in works
        li.work__item
          span.work__btn
          a.work__link(href=`${work.link}`)
            span.work__title= work.title
            img.work__img(src=`images/index/${work.src}` alt=`screenshot of ${work.title}`)
    span.slick-arrow.slick-prev.js-slick-prev--personal
    span.slick-arrow.slick-next.js-slick-next--personal



