section.section
  .l-container
    h1.section__heading
      span.section__headingInner works
  ul.work
    - works = {};
    - works[0] = {title: 'schoo tours'};
    - works[1] = {title: 'Feed Pocket'};
    - works[2] = {title: 'timeline'};
    - works[3] = {title: 'ballxball'};
    each work, i in works
      li.work__item
        a.work__title(href="#")= work.title
        img.work__img(src="https://unsplash.it/1920/300/?image=547")


